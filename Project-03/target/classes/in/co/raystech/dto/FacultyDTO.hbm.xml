<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="in.co.raystech.dto.FacultyDTO" table="ST_FACULTY">
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="increment" />
		</id>
		<property name="firstName" type="java.lang.String">
			<column name="FIRSTNAME" />
		</property>
		<property name="lastName" type="java.lang.String">
			<column name="LASTNAME" />
		</property>
		<property name="gender" type="java.lang.String">
			<column name="GENDER" />
		</property>
		<property name="dOJ" type="java.util.Date" access="field">
			<column name="DOJ" />
		</property>
		<property name="qualification" type="java.lang.String">
			<column name="QUALIFICATION" />
		</property>
		<property name="emailId" type="java.lang.String">
			<column name="EMAILID" />
		</property>
		<property name="mobileNo" type="java.lang.String">
			<column name="MOBILENO" />
		</property>
		<property name="collegeId" type="long">
			<column name="COLLEGEID" />
		</property>
		<property name="collegeName" type="java.lang.String">
			<column name="COLLEGENAME" />
		</property>
		<property name="courseId" type="long">
			<column name="COURSEID" />
		</property>
		<property name="courseName" type="java.lang.String">
			<column name="COURSENAME" />
		</property>
		<property name="subjectId" type="long">
			<column name="SUBJECTID" />
		</property>
		<property name="subjectName" type="java.lang.String">
			<column name="SUBJECTNAME" />
		</property>
		<property name="createdBy" type="java.lang.String"
			length="50">
			<column name="CREATED_BY" />
		</property>
		<property name="modifiedBy" type="java.lang.String"
			length="50">
			<column name="MODIFIED_BY" />
		</property>
		<property name="createdDatetime" type="java.sql.Timestamp">
			<column name="CREATED_DATETIME" />
		</property>
		<property name="modifiedDatetime" type="java.sql.Timestamp">
			<column name="MODIFIED_DATETIME" />
		</property>
	</class>
</hibernate-mapping>
