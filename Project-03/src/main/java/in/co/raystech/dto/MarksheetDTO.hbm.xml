<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="in.co.raystech.dto.MarksheetDTO"
		table="ST_MARKSHEET">
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="increment" />
		</id>
		<property name="rollNo" type="java.lang.String">
			<column name="ROLLNO" />
		</property>
		<property name="studentId" type="long">
			<column name="STUDENTID" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<property name="chemistry" type="int">
			<column name="CHEMISTRY" />
		</property>
		<property name="maths" type="int">
			<column name="MATHS" />
		</property>
		<property name="physics" type="int">
			<column name="PHYSICS" />
		</property>

		<property name="createdBy" type="java.lang.String"
			length="50">
			<column name="CREATED_BY" />
		</property>
		<property name="modifiedBy" type="java.lang.String"
			length="50">
			<column name="MODIFIED_BY" />
		</property>
		<property name="createdDatetime" type="java.sql.Timestamp">
			<column name="CREATED_DATETIME" />
		</property>
		<property name="modifiedDatetime" type="java.sql.Timestamp">
			<column name="MODIFIED_DATETIME" />
		</property>
	</class>
</hibernate-mapping>
